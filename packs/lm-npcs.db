t enlarged = actor.effects.find((fx) => fx.data.label == 'Enlarged');\n\t\tconst updates = [];\n\t\tif (!enlarged) {\n\t\t\ttoken.data.width = 1;\n\t\t\tawait new Promise(async (resolve, reject) => {\n\t\t\t\tsetTimeout(async () => {\n\t\t\t\t\tif (token.data.width == 1) await token.update({ width: 2, height: 2 });\n\t\t\t\t\tresolve();\n\t\t\t\t}, 100);\n\t\t\t});\n\t\t\tactor.items.forEach(async (item) => {\n\t\t\t\tconst melee_str =\n\t\t\t\t\t(item.data.data.actionType == 'mwak' && item.data.data.ability == 'str') ||\n\t\t\t\t\t(!item.data.data.ability && actor.data.data.abilities.str.mod >= actor.data.data.abilities.dex.mod);\n\t\t\t\tif (item.type == 'weapon' && melee_str && !item.getFlag('npcgen', 'damage')) {\n\t\t\t\t\tconst pattern = /(\\d+)(d(\\d+).*)/;\n\t\t\t\t\tconst update = {\n\t\t\t\t\t\t_id: item._id,\n\t\t\t\t\t\t'data.damage': { ...item.data.data.damage },\n\t\t\t\t\t\t'flags.npcgen.damage': item.data.data.damage,\n\t\t\t\t\t};\n\t\t\t\t\tif (item.data.data.damage.versatile) {\n\t\t\t\t\t\tconst match = pattern.exec(item.data.data.damage.versatile);\n\t\t\t\t\t\tif (match) update['data.damage'].versatile = String(Number(match[1]) * 2) + match[2];\n\t\t\t\t\t}\n\t\t\t\t\tconst parts = [];\n\t\t\t\t\titem.data.data.damage.parts.forEach((element) => {\n\t\t\t\t\t\tconst type = item.data.data.damage.parts[0][1];\n\t\t\t\t\t\tconst match = pattern.exec(element[0]);\n\t\t\t\t\t\tif (match) parts.push([String(Number(match[1]) * 2) + match[2], type]);\n\t\t\t\t\t});\n\t\t\t\t\tif (parts.length) update['data.damage'].parts = parts;\n\t\t\t\t\tupdates.push(update);\n\t\t\t\t}\n\t\t\t});\n\t\t} else {\n\t\t\ttoken.data.width = 2;\n\t\t\tawait new Promise(async (resolve, reject) => {\n\t\t\t\tsetTimeout(async () => {\n\t\t\t\t\tif (token.data.width == 2) await token.update({ width: 1, height: 1 });\n\t\t\t\t\tresolve();\n\t\t\t\t}, 100);\n\t\t\t});\n\t\t\tactor.items.forEach(async (item) => {\n\t\t\t\tif (item.getFlag('npcgen', 'damage')) {\n\t\t\t\t\tconst dmg = item.getFlag('npcgen', 'damage');\n\t\t\t\t\tif (dmg) {\n\t\t\t\t\t\tconst update = {\n\t\t\t\t\t\t\t_id: item._id,\n\t\t\t\t\t\t\t'data.damage': dmg,\n\t\t\t\t\t\t\t'flags.npcgen.damage': null,\n\t\t\t\t\t\t};\n\t\t\t\t\t\tupdates.push(update);\n\t\t\t\t\t}\n\t\t\t\t}\n\t\t\t});\n\t\t}\n\t\tif (updates.length) actor.updateEmbeddedDocuments('Item', updates);","folder":null,"sort":0,"permission":{"default":0},"flags":{}}}},"betterRolls5e":{"critRange":{"type":"String","value":null},"critDamage":{"type":"String","value":""},"quickDesc":{"type":"Boolean","value":true,"altValue":true},"quickAttack":{"type":"Boolean","value":true,"altValue":true},"quickSave":{"type":"Boolean","value":true,"altValue":true},"quickDamage":{"type":"Array","value":[],"altValue":[],"context":[]},"quickProperties":{"type":"Boolean","value":true,"altValue":true},"quickCharges":{"type":"Boolean","value":{"use":true,"resource":true,"charge":true},"altValue":{"use":true,"resource":true,"charge":true}},"quickTemplate":{"type":"Boolean","value":true,"altValue":true},"quickOther":{"type":"Boolean","value":true,"altValue":true,"context":""},"quickFlavor":{"type":"Boolean","value":true,"altValue":true},"quickPrompt":{"type":"Boolean","value":false,"altValue":false}},"core":{"sourceId":"Item.1Glyfk1GQzeiouXZ"}},"system":{"description":{"value":"<p>For 1 minute, the duergar magically increases in size, along with anything it is wearing or carrying. While enlarged, the duergar is Large, doubles its damage dice on Strength-based weapon attacks (<b>NOT</b> included in the attacks, but included in the item macro), and makes Strength checks and Strength saving throws with advantage. If the duergar lacks the room to become Large, it attains the maximum size possible in the space available.</p>","chat":"","unidentified":""},"source":"","activation":{"type":"action","cost":1,"condition":""},"duration":{"value":1,"units":"minute"},"target":{"value":null,"width":null,"units":"","type":"self"},"range":{"value"